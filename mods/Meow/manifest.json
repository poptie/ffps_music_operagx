{
    "manifest_version": 3,
    "name": "Meow",
    "description": "No animals were harmed while making this meowsome soundpack.",
    "version": "1.0",
    "icons":
    {
        "512": "icon_512.png"
    },
    "developer":
    {
        "name": "Opera Software"
    },
    "mod":
    {
        "schema_version": 1,
        "payload":
        {
            "keyboard_sounds":
            {
                "TYPING_LETTER":
                [
                    "sounds/CatSounds_Meow1.mp3",
                    "sounds/CatSounds_Meow2.mp3",
                    "sounds/CatSounds_Meow3.mp3",
                    "sounds/CatSounds_Meow4.mp3"
                ],
                "TYPING_ENTER":
                [
                    "sounds/CatSounds_Meow3.mp3"
                ],
                "TYPING_SPACE":
                [
                    "sounds/CatSounds_Meow2.mp3"
                ],
                "TYPING_BACKSPACE":
                [
                    "sounds/CatSounds_Hiss.mp3"
                ]
            }
        }
    }
}