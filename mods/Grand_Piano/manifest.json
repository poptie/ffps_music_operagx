{
    "manifest_version": 3,
    "name": "Grand Piano",
    "description": "Let the melody flow through your keystrokes.",
    "version": "1.1",
    "icons":
    {
        "512": "icon_512.png"
    },
    "developer":
    {
        "name": "Opera Software"
    },
    "mod":
    {
        "schema_version": 1,
        "payload":
        {
            "keyboard_sounds":
            {
                "TYPING_LETTER":
                [
                    "sounds/Grand_Piano_-_keyboard_effects-001.mp3",
                    "sounds/Grand_Piano_-_keyboard_effects-002.mp3",
                    "sounds/Grand_Piano_-_keyboard_effects-003.mp3",
                    "sounds/Grand_Piano_-_keyboard_effects-004.mp3",
                    "sounds/Grand_Piano_-_keyboard_effects-005.mp3"
                ],
                "TYPING_ENTER":
                [
                    "sounds/Grand_Piano_-_keyboard_effects-006.mp3"
                ],
                "TYPING_SPACE":
                [
                    "sounds/Grand_Piano_-_keyboard_effects-007.mp3"
                ],
                "TYPING_BACKSPACE":
                [
                    "sounds/Grand_Piano_-_keyboard_effects-backspace.mp3"
                ]
            }
        }
    }
}